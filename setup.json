{
    "name": "aiida-jdftx",
    "author": "Jason Yu",
    "author_email": "morty.yeu@gmail.com",
    "description": "AiiDA plugin for JDFTx",
    "url": "https://github.com/unkcpz/aiida-jdftx",
    "license": "MIT",
    "classifiers": [
        "Programming Language :: Python",
        "Intended Audience :: Science/Research",
        "License :: OSI Approved :: MIT License",
        "Natural Language :: English",
        "Framework :: AiiDA"
    ],
    "version": "0.1.0a0",
    "entry_points": {
        "aiida.calculations": [
            "jdftx = aiida_jdftx.calculations:JdftxCalculation"
        ],
        "aiida.parsers": [
            "jdftx = aiida_jdftx.parsers:JdftxParser"
        ],
        "aiida.workflows": [
            "jdftx.base = aiida_jdftx.workflows.base:JdftxBaseWorkChain"
        ]
    },
    "include_package_data": true,
    "setup_requires": ["reentry"],
    "reentry_register": true,
    "install_requires": [
        "aiida-core>=1.5.0,<2.0.0",
        "sqlalchemy<1.4",
        "six",
        "voluptuous",
        "aiida-pseudo~=0.6"
    ],
    "extras_require": {
        "testing": [
            "pgtest~=1.3.1",
            "wheel~=0.31",
            "coverage",
            "pytest~=3.6,<5.0.0",
            "pytest-regressions~=2.2",
            "pytest-cov"
        ],
        "pre-commit": [
            "astroid<2.5",
            "pre-commit~=2.2",
            "pylint~=2.2.0"
        ],
        "docs": [
            "sphinx",
            "sphinxcontrib-contentui",
            "sphinxcontrib-details-directive; python_version>='3.0'",
            "sphinx-rtd-theme"
        ]
    }
}
